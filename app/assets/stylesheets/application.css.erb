.article_header_block .detail_wrapper {
  display: none;
}

.tsumiki-blog-entry-head {
  max-width: 685px;
}

.tsumiki-blog-entry-head .article_header_block {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding: 22px;
  background: #fff;
  margin: 0 0 10px;
  position: relative;
  -webkit-border-radius: 3px;
  border-radius: 3px;
}

.tsumiki-blog-entry-head .article_header_block:before {
  content: "";
  display: block;
  width: 0;
  height: 0;
  border: 10px solid transparent;
  border-top-color: #000;
  border-left-color: #000;
  position: absolute;
  top: 0;
  left: 0;
  -webkit-border-radius: 3px 0 0;
  border-radius: 3px 0 0;
}

.tsumiki-blog-entry-head .article_header_block .article_link {
  position: relative;
  z-index: 4;
  display: block;
}

.tsumiki-blog-entry-head .article_header_block .article_link:hover {
  text-decoration: none;
}

.tsumiki-blog-entry-head .article_header_block .article_link:hover+.hover_border {
  display: block;
}

.tsumiki-blog-entry-head .article_header_block .article_link time {
  top: 0;
  left: 0;
}

.tsumiki-blog-entry-head .article_header_block h1 {
  font-size: 21px;
  line-height: 1.5;
  margin: 0;
  padding: 20px 0;
}

.tsumiki-blog-entry-head .article_header_block time {
  position: absolute;
  z-index: 4;
  top: 22px;
  left: 22px;
  line-height: 1;
  font-family: "Fredoka One" cursive;
  -webkit-font-smoothing: antialiased;
  font-size: 16px;
}

.tsumiki-blog-entry-head .article_header_block .detail_wrapper {
  display: block;
  position: relative;
  z-index: 4;
  text-align: right;
}

.tsumiki-blog-entry-head .article_header_block .detail_wrapper>* {
  vertical-align: middle;
}

.tsumiki-blog-entry-head .article_header_block .detail_wrapper .author_pic {
  width: 30px;
  height: auto;
  margin: 0 0 0 .5em;
  vertical-align: middle;
}

