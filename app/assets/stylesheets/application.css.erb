body,html {
  height: 100%;
}

.control {
  position: absolute;
  width: 100%;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  background: rgba(0,0,0,.1);
  z-index: 1;
}

.control h1 {
  margin: 0;
  padding: 4px 8px;
  font-weight: lighter;
  font-size: 1em;
}

.wrapper {
  width: 100%;
  height: 100%;
  padding: 36px 0 0;
  display: table;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.header {
  padding-left: 8px;
  position: relative;
}

.delete {
  float: right;
}

.list {
  width: 25%;
  float: left;
}

.text {
  width: 75%;
  float: left;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  margin: 0;
  padding: 0;
  border: none;
  outline: 0;
}

textarea {
  width: 100%;
  height: 100%;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  outline: 0;
  border: 0;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding: 8px;
}

.side {
  display: table-cell;
  vertical-align: top;
  height: 100%;
  width: 20%;
  overflow: auto;
  padding-bottom: 8px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  border-right: solid 8px transparent;
}

.noteList {
  list-style: none;
  padding: 0;
  border-top: 1px solid #ccc;
  margin: 8px 0 0;
  overflow: auto;
  height: 95%;
}

.noteList li {
  position: relative;
  line-height: 3;
  cursor: pointer;
  border-bottom: 1px solid #ccc;
}

.noteList li.ac {
  background: rgba(0,0,0,.1);
}

.noteList .checkbox {
  margin: 0 0 0 5%;
  width: 10%;
  display: block;
  position: absolute;
  z-index: 1;
  top: 50%;
  -webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -o-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
}

.noteList .title {
  display: inline-block;
  vertical-align: middle;
  width: 80%;
  line-height: 1.5;
  padding-left: 20%;
}

.noteList .title-editor {
  display: none;
  position: absolute;
  width: 70%;
  top: 30px;
  left: 30px;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  margin: 0;
  padding: 8px;
  border: 0;
  outline: 0;
  -webkit-box-shadow: 1px 1px 0 2px #999;
  box-shadow: 1px 1px 0 2px #999;
  z-index: 2;
}

.noteList .created {
  line-height: 2;
  display: block;
  text-align: right;
  padding-right: 8px;
  color: #bdc3c7;
}

.textarea-rapper {
  width: 45%;
  height: 100%;
  display: table-cell;
  border-right: solid 8px transparent;
  vertical-align: top;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.preview-wrapper {
  width: 35%;
  height: 100%;
  overflow: auto;
  display: table-cell;
  vertical-align: top;
  border-right: solid 8px transparent;
  padding-bottom: 8px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.preview-btns {
  position: relative;
  margin-bottom: 8px;
}

.preview-btns label {
  width: auto;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  float: right;
  line-height: 1.7;
}

.preview-inner {
  display: none;
  height: 95%;
  overflow: auto;
}
